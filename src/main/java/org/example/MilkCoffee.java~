package org.example;

public class MilkCoffee implements Coffee {
    private final double price = CoffeeType.MILK_COFFEE.getPrice();
    private final String description = CoffeeType.MILK_COFFEE.getDescription();

    public MilkCoffee() {}

    public double getPrice() {
        return price;
    }

    public String getDescription() {
        return description;
    }
}
